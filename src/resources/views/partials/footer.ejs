<footer id="footer">
    <!-- Footer Main  -->
    <div class="footer_center">
        <div class="container">
            <div class="text-info text-center">
                <div class="widget-logo">
                    <div class="text">
                        <p class="mb-2">
                            <strong class="text-uppercase">
                                Đại lý Supper Admin - <%= agency.name %>
                            </strong>
                        </p>
                        <p class="mb-2">
                            Địa chỉ: <%= agency.homeAddress %>
                        </p>
                        <p class="mb-2">
                            Điện thoại: <%= JSON.parse(agency.contact).phone.replace(/(\d{4})(\d{3})(\d{3})/, "$1.$2.$3") %> | Email: <%= JSON.parse(agency.contact).email %> | Website: <%= agency.website %>
                        </p>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Footer Main -->
    <!-- Bottom -->
    <div class="footer_bottom">
        <div class="container">
            <div class="row">

                <div class="col-md-12">
                    <span>Bản quyền © 2017 Đại lý <%= agency.name %>. Tất cả quyền được bảo vệ.</span>
                    <span class="pull-right">Designed By Biso.vn</span>
                </div>

            </div>

        </div>
    </div>
    <!-- Bottom -->

</footer>

